                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <!-- [ form-element ] start -->
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="mb-3">
                                    <label class="form-label">Mã đại lý: *</label>
                                    <input type="text" class="form-control p-2" placeholder="Mã nhà vườn" id="FarmCode">
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="mb-3 pe-2">
